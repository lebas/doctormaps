<p id="notice"><%= notice %></p>

<h1>Listing Dm_clinicas</h1>

<table>
  <thead>
    <tr>
      <th>Especialidade</th>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Telefone</th>
      <th>Logotipo</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Inicio atendimento</th>
      <th>Terminio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dm_clinicas.each do |dm_clinica| %>
      <tr>
        <td><%= dm_clinica.especialidade %></td>
        <td><%= dm_clinica.nome %></td>
        <td><%= dm_clinica.endereco %></td>
        <td><%= dm_clinica.bairro %></td>
        <td><%= dm_clinica.cep %></td>
        <td><%= dm_clinica.cidade %></td>
        <td><%= dm_clinica.estado %></td>
        <td><%= dm_clinica.telefone %></td>
        <td><%= dm_clinica.logotipo %></td>
        <td><%= dm_clinica.latitude %></td>
        <td><%= dm_clinica.longitude %></td>
        <td><%= dm_clinica.inicio_atendimento %></td>
        <td><%= dm_clinica.terminio %></td>
        <td><%= link_to 'Show', dm_clinica %></td>
        <td><%= link_to 'Edit', edit_dm_clinica_path(dm_clinica) %></td>
        <td><%= link_to 'Destroy', dm_clinica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dm clinica', new_dm_clinica_path %>
