<p id="notice"><%= notice %></p>

<h1>Listing Dm_especialidades</h1>

<table>
  <thead>
    <tr>
      <th>Especialidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dm_especialidades.each do |dm_especialidade| %>
      <tr>
        <td><%= dm_especialidade.especialidade %></td>
        <td><%= link_to 'Show', dm_especialidade %></td>
        <td><%= link_to 'Edit', edit_dm_especialidade_path(dm_especialidade) %></td>
        <td><%= link_to 'Destroy', dm_especialidade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

