<p id="notice"><%= notice %></p>

<h1>Listing Dm_medicos</h1>

<table>
  <thead>
    <tr>
      <th>Especialidade</th>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Crm</th>
      <th>Telefone</th>
      <th>Logotipo</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Inicio atendimento</th>
      <th>Terminio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dm_medicos.each do |dm_medico| %>
      <tr>
        <td><%= dm_medico.especialidade %></td>
        <td><%= dm_medico.nome %></td>
        <td><%= dm_medico.endereco %></td>
        <td><%= dm_medico.bairro %></td>
        <td><%= dm_medico.cep %></td>
        <td><%= dm_medico.cidade %></td>
        <td><%= dm_medico.estado %></td>
        <td><%= dm_medico.crm %></td>
        <td><%= dm_medico.telefone %></td>
        <td><%= dm_medico.logotipo %></td>
        <td><%= dm_medico.latitude %></td>
        <td><%= dm_medico.longitude %></td>
        <td><%= dm_medico.inicio_atendimento %></td>
        <td><%= dm_medico.terminio %></td>
        <td><%= link_to 'Show', dm_medico %></td>
        <td><%= link_to 'Edit', edit_dm_medico_path(dm_medico) %></td>
        <td><%= link_to 'Destroy', dm_medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

